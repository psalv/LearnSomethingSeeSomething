{
  "name":"Learn Something, See Something",
  "version":"1.0",
  "manifest_version":2,
  "description":"We grow a little every day.",

  "chrome_url_overrides":{
    "newtab": "NewTab.html"
  },


  "browser_action": {
    "default_icon":"res/icon1.png",
    "default_popup":"popup.html"
  },

  "background": {
        "persistent": false,
        "scripts": ["framework/js/jquery-3.1.0.js", "main.js", "popup.js"]
    },

  "permissions":[
    "http://api.wordnik.com/v4/words.json/*"
  ]
}